<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="52dp"
    android:descendantFocusability="blocksDescendants"
    android:orientation="horizontal"
    android:paddingBottom="10dp"
    android:paddingLeft="5dp"
    android:paddingTop="10dp" >

    <RelativeLayout
        android:id="@+id/Layout_fix"
        android:layout_width="38dp"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/tv_PotNo_head"
            android:layout_width="38dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="0dp"
            android:text="槽号"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/text_black"
            android:textSize="11sp" />

        <ImageView
            android:id="@+id/iv_PotNo"
            android:layout_width="@dimen/sort_image_width"
            android:layout_height="@dimen/sort_image_hight"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/sort" />
    </RelativeLayout>

    <com.hewaiming.ALWorkInfo.HScrollListView.InterceptScrollContainer_Header
        android:id="@+id/scroollContainter"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_toRightOf="@id/Layout_fix"
        android:focusable="false" >

        <com.hewaiming.ALWorkInfo.HScrollListView.MyHScrollView
            android:id="@+id/horizontalScrollView1_head"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:focusable="false"
            android:scrollbars="none" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:focusable="false"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/tv_Ddate_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="2dp"
                    android:text="    日      期 "
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="10sp" />

                <ImageView
                    android:id="@+id/iv_Ddate"
                    android:layout_width="@dimen/sort_image_width"
                    android:layout_height="@dimen/sort_image_hight"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/sort" />

                <TextView
                    android:id="@+id/tv_ALCnt_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="2dp"
                    android:text="出铝量"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <ImageView
                    android:id="@+id/iv_ALCnt"
                    android:layout_width="@dimen/sort_image_width"
                    android:layout_height="@dimen/sort_image_hight"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/sort" />

                <TextView
                    android:id="@+id/tv_LSP_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="2dp"
                    android:text="铝水平"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <ImageView
                    android:id="@+id/iv_LSP"
                    android:layout_width="@dimen/sort_image_width"
                    android:layout_height="@dimen/sort_image_hight"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/sort" />

                <TextView
                    android:id="@+id/tv_DJZSP_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="2dp"
                    android:text="电解质水平"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <ImageView
                    android:id="@+id/iv_DJZSP"
                    android:layout_width="@dimen/sort_image_width"
                    android:layout_height="@dimen/sort_image_hight"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/sort" />

                <TextView
                    android:id="@+id/tv_DJWD_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="2dp"
                    android:text="电解温度"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <ImageView
                    android:id="@+id/iv_DJWD"
                    android:layout_width="@dimen/sort_image_width"
                    android:layout_height="@dimen/sort_image_hight"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/sort" />

                <TextView
                    android:id="@+id/tv_FZB_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="2dp"
                    android:text=" 分子比"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <ImageView
                    android:id="@+id/iv_FZB"
                    android:layout_width="@dimen/sort_image_width"
                    android:layout_height="@dimen/sort_image_hight"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/sort" />

                <TextView
                    android:id="@+id/tv_FeCnt_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="2dp"
                    android:text="  铁含量"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <ImageView
                    android:id="@+id/iv_FeCnt"
                    android:layout_width="@dimen/sort_image_width"
                    android:layout_height="@dimen/sort_image_hight"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/sort" />

                <TextView
                    android:id="@+id/tv_SiCnt_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="2dp"
                    android:text="  硅含量"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <ImageView
                    android:id="@+id/iv_SiCnt"
                    android:layout_width="@dimen/sort_image_width"
                    android:layout_height="@dimen/sort_image_hight"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/sort" />

                <TextView
                    android:id="@+id/tv_ALOCnt_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="2dp"
                    android:text="  氧化铝浓度"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="10sp" />

                <ImageView
                    android:id="@+id/iv_ALOCnt"
                    android:layout_width="@dimen/sort_image_width"
                    android:layout_height="@dimen/sort_image_hight"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/sort" />

                <TextView
                    android:id="@+id/tv_CaFCnt_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="2dp"
                    android:text="氟化钙含量"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="10sp" />

                <ImageView
                    android:id="@+id/iv_CaFCnt"
                    android:layout_width="@dimen/sort_image_width"
                    android:layout_height="@dimen/sort_image_hight"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/sort" />

                <TextView
                    android:id="@+id/tv_MgCnt_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="2dp"
                    android:text="镁含量"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="10sp" />

                <ImageView
                    android:id="@+id/iv_MgCnt"
                    android:layout_width="@dimen/sort_image_width"
                    android:layout_height="@dimen/sort_image_hight"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/sort" />

                <TextView
                    android:id="@+id/tv_MLSP_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="2dp"
                    android:text="侧壁温度"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="10sp" />

                <ImageView
                    android:id="@+id/iv_MLSP"
                    android:layout_width="@dimen/sort_image_width"
                    android:layout_height="@dimen/sort_image_hight"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/sort" />

                <TextView
                    android:id="@+id/tv_LDYJ_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="2dp"
                    android:text="阳极压降"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="10sp" />

                <ImageView
                    android:id="@+id/iv_LDYJ"
                    android:layout_width="@dimen/sort_image_width"
                    android:layout_height="@dimen/sort_image_hight"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/sort" />

                <TextView
                    android:id="@+id/tv_JHCL_head"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="2dp"
                    android:text="计划出铝量"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="10sp" />

                <ImageView
                    android:id="@+id/iv_JHCL"
                    android:layout_width="@dimen/sort_image_width"
                    android:layout_height="@dimen/sort_image_hight"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/sort" />
            </LinearLayout>
        </com.hewaiming.ALWorkInfo.HScrollListView.MyHScrollView>
    </com.hewaiming.ALWorkInfo.HScrollListView.InterceptScrollContainer_Header>

</RelativeLayout>