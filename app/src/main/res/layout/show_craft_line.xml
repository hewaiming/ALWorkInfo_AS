<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/base_bg_gray"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.hewaiming.ALWorkInfo.ui.ParamsActivity" >

    <include
        android:layout_width="match_parent"
        android:layout_height="40dp"
        layout="@layout/include_back" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_SetV"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_WorkV"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_AvgV"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_Noise"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_DYBTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_ALF"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_AeCnt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_YhlCnt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_ALCntZSL"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_ALCnt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_FeCnt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_SiCnt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_FZB"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_DJWD"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_LSP"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />

            <com.github.mikephil.charting.charts.LineChart
                android:id="@+id/chart_DJZSP"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>