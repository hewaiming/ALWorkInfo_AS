<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:descendantFocusability="blocksDescendants"
    android:orientation="horizontal"
    android:padding="5dp" >

    <TextView
        android:id="@+id/tv_PotNo"
        android:layout_width="38dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="5dp"
        android:text="槽号"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/text_black"
        android:textSize="11sp" />

    <com.hewaiming.ALWorkInfo.HScrollListView.InterceptScrollContainer
        android:id="@+id/scroollContainter"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_toRightOf="@id/tv_PotNo"
        android:focusable="false" >

        <com.hewaiming.ALWorkInfo.HScrollListView.MyHScrollView
            android:id="@+id/horizontalScrollView1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:focusable="false"
            android:scrollbars="none" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:focusable="false"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/tv_PotSt"
                    android:layout_width="36dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:text="槽状态"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/tv_RunTime"
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:text="运行时间"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />
                 <TextView
                    android:id="@+id/tv_YHLCnt"
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="5dp"
                    android:text="加料量"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />
                  <TextView
                    android:id="@+id/tv_JLCnt"
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="5dp"
                    android:text="加量次数"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/tv_SetV"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="5dp"
                    android:text="设定电压"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/tv_RealSetV"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="5dp"
                    android:text="实设电压"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/tv_WorkV"
                    android:layout_width="42dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="5dp"
                    android:text="工作电压"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/tv_AverageV"
                    android:layout_width="44dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="5dp"
                    android:text="平均电压"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/tv_AeV"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:text="效应电压"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/tv_AeTime"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:text="效应时长"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/tv_AeCnt"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="5dp"
                    android:text="效应次数"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/tv_DybTime"
                    android:layout_width="46dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="5dp"
                    android:text="电压摆时长"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/tv_ALFCnt"
                    android:layout_width="56dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="5dp"
                    android:text="氟化铝下料量"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/tv_ALCntZSL"
                    android:layout_width="54dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="14dp"
                    android:layout_marginRight="5dp"
                    android:text="出铝指示量"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />

                <TextView
                    android:id="@+id/tv_Ddate"
                    android:layout_width="64dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:text="日期"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="@color/text_black"
                    android:textSize="11sp" />
            </LinearLayout>
        </com.hewaiming.ALWorkInfo.HScrollListView.MyHScrollView>
    </com.hewaiming.ALWorkInfo.HScrollListView.InterceptScrollContainer>

</RelativeLayout>